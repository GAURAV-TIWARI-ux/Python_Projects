    "# Find the factorial of n using for loop.\n",
    "def calculate_factorial(n):\n",
    "    fact=1\n",
    "    for i in range(1,n+1):\n",
    "        fact=fact*i\n",
    "    return fact\n",
    "\n",
    "print(calculate_factorial(6))\n",
    "print(calculate_factorial(5))\n",
    "print(calculate_factorial(4))\n",
    "print(calculate_factorial(12))"
   ]
  }
