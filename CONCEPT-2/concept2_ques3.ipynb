    "# Write a function that takes n as input\n",
    "# and returns the sum of all numbers from 1 to n\n",
    "def calculate_sum(n):\n",
    "  total = 0\n",
    "  for i in range(1,n+1):\n",
    "    total=total+i\n",
    "  return total\n",
    "\n",
    "print(calculate_sum(7))\n",
    "print(calculate_sum(10))\n",
    "print(calculate_sum(8))\n",
    "print(calculate_sum(15))"
   ]
  }
