    "# Given a positive integer, return the even number of digits in it.\n",
    "n=int(input(\"enter the number:\"))\n",
    "if n<0:\n",
    "    print('enter positive no.')\n",
    "m=str(n)\n",
    "count=0\n",
    "for i in m:\n",
    "    if int(i)%2==0:\n",
    "        count=count+1\n",
    "print(\"the no. of digits in number are\",count)"
   ]
